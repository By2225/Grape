fun Int main() {

    Edge<String> aEdge;

    Node<Int> start = '0';

    String input = "bbbbbbbbbbb";
    Graph<Int, String> dfa = << start  -"b"-  '1'  -"b"-  '0'  -"b"-  '1' >>;
    Int token = 0;
    Int i = 0;
    Bool found = False;

    String character;
    List<Edge<String> > transitions;

    Node<Int> state = start;
    while( token < str_size(input)) { 			// String size

	character = get_char(input, token);		// Get char at token
        transitions = get_outgoing(state); 			// Get outgoing edges

	i = 0;
	while ((i < size(transitions)) and (found == False)) { 			// List size
	    aEdge = transitions[i]; 	// Get list item
	    if (str_equal(aEdge.val, character)) { 	// Get list string
		state = get_to(aEdge);			// Get edge destination
                found = True;
	    }
	    i = i + 1;
	}
        if ( found == False){
            prints("REJECT");
            return 0;
        }
        found = False;
	token = token + 1;
    }

    if (state.val == 1) {		// Get accepting
        prints("ACCEPT");
    } else {
        prints("REJECT");
    }

    return 0;
}
